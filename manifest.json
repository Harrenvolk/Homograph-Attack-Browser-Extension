{
    "name": "Homograph Attack Detector",
    "description": "Detects and flags possible homograph attacks using a multi-pronged approach",
    "version": "0.1.0",
    "manifest_version": 3,
    "icons": {
        "128": "/images/icon-128x128.png"
    },
    "background": {
        "service_worker": "background-scripts/background.js"
    },
    "action": {
        "default_icon": {
            "128": "/images/icon.png"
        }
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "tabs"
    ]
}